#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n,m;
    cin>>n>>m;
    int divisible_sum = 0;
    int not_divisible_sum = 0;
    for(int i=1;i<=m;i++)
    {
        if(i%n == 0)
        divisible_sum += i;
        else
        not_divisible_sum += i;
    }
    cout<<not_divisible_sum - divisible_sum;

    return 0;
}
