

#include <bits/stdc++.h>

using namespace std;
void ans(int arr[],int n)
{
    unordered_map<int,int> mpp;
    
    for(int i=0;i<n;i++)
    mpp[arr[i]]++;
    
    for(auto i:mpp)
    cout<<i.first<<" "<<i.second<<" "<<endl;
    
    
}

int main()
{
    int arr[] = {10,5,10,15,10,5};
    int n = sizeof(arr) / sizeof(arr[0]);
    ans(arr, n);

    return 0;
}
