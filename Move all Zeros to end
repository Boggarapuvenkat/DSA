

#include <bits/stdc++.h>
using namespace std;
vector<int> solve(int arr[],int n)
{
    int k = 0;
    vector<int> v;
    for(int i=0;i<n;i++)
    {
        if(arr[i] != 0)
        {
            v.push_back(arr[i]);
            k++;
        }
    }
    for(int i=k;i<n;i++)
    v.push_back(0);
   
   return v;
}
int main()
{
    int arr[] = {10,20,30,0,1,0,2};
    int n = sizeof(arr)/sizeof(arr[0]);
    vector<int> ans = solve(arr,n);
    
    for(int i=0;i<n;i++)
    cout<<ans[i]<<" ";

    return 0;
}
